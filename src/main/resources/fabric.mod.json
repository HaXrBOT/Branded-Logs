{
	"schemaVersion": 1,
	"id": "brandedlogs",
	"version": "${version}",
	"name": "Branded Logs",
	"description": "Lets modpack authors print information about their modpack into the logs & crash logs!",
	"authors": [
		"HaXr"
	],
	"contact": {
		"homepage": "https://www.curseforge.com/minecraft/mc-mods/branded-logs",
		"sources": "https://github.com/HaXrBOT/Branded-Logs-Fabric"
	},
	"license": "LGPL 3.0",
	"icon": "assets/brandedlogs/icon.png",
	"environment": "*",
	"entrypoints": {
		"main": [
			"haxr.brandedlogs.BrandedLogsMod"
		],
		"modmenu": [
			"haxr.brandedlogs.config.ModMenuIntegration"
		]
	},
	"mixins": [
		"mixins.json"
	],
	"depends": {
		"fabricloader": ">=0.15.10",
		"minecraft": "~1.20.1",
		"java": ">=17",
		"fabric-api": "*"
	},
	"custom": {
		"modmenu": {
			"links": {
				"modmenu.discord": "https://discord.gg/Kss5gBgeDA"
			}
		}
